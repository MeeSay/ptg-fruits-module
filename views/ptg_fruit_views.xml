<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_form_ptg_fruit" model="ir.ui.view">
            <field name="name">ptg.fruit.form</field>
            <field name="model">ptg.fruit</field>
            <field name="arch" type="xml">
                <form string="PTG Fruit">
                    <sheet>
                        <group>
                            <field name="name" />
                            <field name="average" />
                            <field name="imgs" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_tree_ptg_fruit" model="ir.ui.view">
            <field name="name">ptg.fruit.tree</field>
            <field name="model">ptg.fruit</field>
            <field name="arch" type="xml">
                <tree string="PTG Fruit">
                    <field name="name" />
                    <field name="average" />
                    <field name="imgs" />
                </tree>
            </field>
        </record>
    </data>
</odoo>